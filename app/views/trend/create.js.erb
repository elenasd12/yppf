
$('#search_results').html("<%= escape_javascript (render(partial: 'search_results')) %>");

function goBack(){
  window.history.back();
}

//<!--High Charts bar chart  -->
$(function () {
    $('#expense_bar').highcharts({
        chart: {
            type: 'column'
        },
        title: {
            text: 'Expense Comparison'
        },
        xAxis: {
            categories: <%=@months.to_json.html_safe %>
        },
        yAxis: {
            title: {
                text: 'Money Spent'
                // y axis - how much money
            }
        },
        series: [{
            // months
            name: '<%= @selected_expense %>',
            data: <%= @values.to_json.html_safe %>
        }]
    });
});
